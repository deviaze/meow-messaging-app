{
  "luau-lsp.fflags.override": {
    "LuauTypeSolverRelease": "654",
    "LuauNewSolverPopulateTableLocations": "true",
    "LuauNewSolverPrePopulateClasses": "true",
    "LuauSolverV2": "false"
  },
  "code-runner.executorMap": {
    "luau": "seal run client"
  },
  "workbench.editor.customLabels.patterns": {
    "**/old/src/**/*.luau": "${dirname(2)}/${filename}.luau",
    "**/init.luau": "${dirname}/"
  },
  "luau-lsp.types.definitionFiles": [
    "./typedefs/globals.d.luau"
  ],
  "luau-lsp.completion.imports.suggestRequires": true,
  "stylua.targetReleaseVersion": "latest",
  "luau-lsp.require.directoryAliases": {
    "@workspace": "./",
    "@std": "./typedefs/std/"
  }
}